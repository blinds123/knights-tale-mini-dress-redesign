{
  "name": "knights-tale-mini-dress-redesign",
  "version": "1.0.0",
  "description": "Highly-customized, conversion-optimized landing page for Knights Tale Mini Dress with custom animations and micro-interactions",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:css && npm run build:js && npm run optimize:images",
    "build:css": "postcss src/styles/*.css -d public/assets/css/",
    "build:js": "rollup -c rollup.config.js",
    "optimize:images": "node scripts/optimize-images.js",
    "preview": "vite preview",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "keywords": [
    "fashion",
    "ecommerce",
    "landing-page",
    "custom-animations"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0",
    "vite": "^6.0.7"
  },
  "dependencies": {
    "lenis": "^1.0.42"
  }
}
